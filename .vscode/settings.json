{
    /* vscode */
    /** files **/
    "files.exclude": {
        "**/.git": false,
        "**/.target": false,
        "**/.classpath": false,
    },
    /** editor **/
    "editor.fontSize": 16,
    "editor.fontWeight": "400",
    "editor.fontLigatures":  true,
    "editor.fontFamily":
        //"Apercu Mono"
        //"Fira Code"
        //"Input Mono"
        //"Dank Mono"
        //"Gintronic"
        //"Monoid"
        //"Hack"
        //"Victor Mono"
        //"DejaVu Sans Mono"
        //"MonoLisa"
        //"Consolas, 'Courier New', monospace"
        //"SourceCodePro-Regular"
        "Source Code Pro, Mono Lisa, Victor Mono, Input Mono, Fira Code, Inconsolata, MonoId, Consolas, Courier New, monospace"
        //"VT323, Mozart NBP, Mayfair NBP, Silverfinster"
        //"Daedra, Dragon Script"
        //"mason chronicles"
        //"Manic Miner, Mayfair NBP, VT323 Regular"
        //"Manic Maner Regular"
    ,
    /* extensions */
    /** drawio **/
    "hediet.vscode-drawio.offline": true,
    "hediet.vscode-drawio.theme": "dark",
    "hediet.vscode-drawio.online-url": "https://embed.diagrams.net/",
    "hediet.vscode-drawio.plugins": [],
    "hediet.vscode-drawio.knownPlugins": [],
    "hediet.vscode-drawio.customLibraries": [],
    "hediet.vscode-drawio.customColorSchemes": [],
    "hediet.vscode-drawio.customFonts": [ "Consolas" ],
    /** dashboard **/
    "dashboard.openOnStartup": "empty workspace",
    "dashboard.projectTileWidth": 350,
    "dashboard.displayProjectPath": true,
    "dashboard.showAddProjectButtonTile": false,
    /** java **/
    "java.home": "f:/cdev/jdk",
    "java.trace.server": "off",
    "java.server.launchMode": "Standard",
    "java.contentProvider.preferred": "",
    /*** project ***/
    "java.project.outputPath": "${workspaceFolder}/bin/",
    "java.project.exportJar.targetPath": "${workspaceFolder}/bin/${workspaceFolderBasename}.jar",
    "java.project.sourcePaths": [ "${workspaceFolder}/src", "${workspaceFolder}/src/main", "${workspaceFolder}/src/test" ],
    "java.project.referencedLibraries": [
        "lib/**/*.jar"
    ],
    "java.project.resourceFilters": [
        "node_modules",
        ".git"
    ],
    /*** configuration ***/
    "java.configuration.checkProjectSettingsExclusions": false,
    "java.configuration.updateBuildConfiguration": "disabled",
    "java.configuration.maven.userSettings": "${workspaceFolder}/maven/settings.xml",
    "java.configuration.maven.globalSettings": "${workspaceFolder}/maven/settings.xml",
    /*** completion ***/
    "java.completion.enabled": true,
    "java.completion.overwrite": true,
    /*** format ***/
    "java.format.enabled": true,
    "java.format.onType.enabled": true,
    "java.format.comments.enabled": true,
    "java.format.settings.profile": "",
    "java.format.settings.url": "",
    /*** debug ***/
    "java.debug.settings.forceBuildBeforeLaunch": true,
    "java.debug.settings.showToString": true,
    "java.autobuild.enabled": false,
    "java.debug.settings.showHex": true,
    "java.dependency.packagePresentation": "hierarchical",
    "java.eclipse.downloadSources": false,
    /*** import ***/
    "java.import.maven.enabled": true,
    /*** codegen ***/
    "java.codeGeneration.generateComments": true,
    "java.templates.fileHeader": [
        "/* package */",
        "package ${package_name};",
        "/* include */",
        "import ${package_name}.*;",
        "/* typedef */",
        "/* endfile */",
    ],
    "java.templates.typeComment": [
        "/* ${type_name}",
        " * > Description:",
        " * -> ;",
        "*/"
    ],
    /** maven **/
    "maven.view": "hierarchical",
    "maven.terminal.customEnv": [],
    "maven.terminal.favorites": [],
    "maven.terminal.useJavaHome": true,
    "maven.executable.options": "",
    "maven.executable.preferMavenWrapper": false,
    /** vsintellicode **/
    "vsintellicode.java.completionsEnabled": true,
    "vsintellicode.modelDownloadPath": null,
}
/* endfile */